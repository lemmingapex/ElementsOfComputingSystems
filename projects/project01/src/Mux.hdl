/** 
 * Multiplexor.  If sel=0 then out = a else out = b.
 */

CHIP Mux {

    IN  a, b, sel;
    OUT out;

    PARTS:
    Not(in=sel, out=nsel);
    And(a=a, b=nsel, out=t1);
    And(a=b, b=sel, out=t2);
    Or(a=t1, b=t2, out=out);
}
