/**
 *  Exclusive-or gate.  out = a xor b.
 */

CHIP Xor {

    IN  a, b;
    OUT out;

    PARTS:
    Not(in=a, out=nota);
    Not(in=b, out=notb);
    And(a=a, b=notb, out=t1);
    And(a=nota, b=b, out=t2);
    Or(a=t1, b=t2, out=out);
}
